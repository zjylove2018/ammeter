<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.zjy.laoyan.ammeter.mapper.ShowMoreInfoMapper">
    <select id="selectInfo" resultType="DataInfo" parameterType="java.lang.Integer">
        SELECT * FROM
            ammeter_info
        where
            id = #{id,jdbcType=INTEGER}
        and
            isShow = "10"
    </select>
</mapper>